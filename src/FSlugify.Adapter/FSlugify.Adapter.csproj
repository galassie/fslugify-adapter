<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.1</TargetFramework>
  </PropertyGroup>

  <PropertyGroup>
    <PackageId>FSlugify.Adapter</PackageId>
		<Title>FSlugify: a minimalistic slug generator library</Title>
    <Version>1.1.0</Version>
    <Description>Ad Object-Oriented adapter to the functional FSlugify library.</Description>
		<Authors>Enrico Galassi</Authors>
		<PackageLicenseUrl>https://raw.githubusercontent.com/galassie/fslugify-adapter/master/LICENSE.md</PackageLicenseUrl>
		<PackageRequireLicenseAcceptance>False</PackageRequireLicenseAcceptance>
		<NoPackageAnalysis>True</NoPackageAnalysis>
		<PackageTags>slug;slugify;C#;csharp;</PackageTags>
    <PackageIconUrl>https://raw.githubusercontent.com/galassie/fslugify-adapter/master/assets/fslugify-alt-200x200.png</PackageIconUrl>
		<RepositoryUrl>https://github.com/galassie/fslugify-adapter</RepositoryUrl>
		<IncludeSymbols>True</IncludeSymbols>
    <RepositoryType>git</RepositoryType>
		<SymbolPackageFormat>snupkg</SymbolPackageFormat>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="FSlugify" Version="1.1.0" />
  </ItemGroup>

</Project>
